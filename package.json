{
  "name": "react-transformer",
  "publisher": "jamespan",
  "displayName": "React Transformer",
  "description": "支持 JSX 结构的多种快捷操作，包括用新标签包裹、与兄弟节点交换、创建 forwardRef、移除包裹标签、批量移除内容、创建 & 表达式、条件表达式等。旨在提升 React 开发效率，简化常见的组件结构调整和重构流程。",
  "version": "0.0.4",
  "icon": "assets/icon.png",
  "author": {
    "name": "James Pan",
    "email": "panjiang@gmail.com",
    "wechat": "panjiang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-transformer/react-transformer.git"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [],
    "submenus": [
      {
        "id": "react-transformer.menu",
        "label": "React Transformer"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "react-transformer.menu",
          "group": "react-transformer"
        }
      ],
      "react-transformer.menu": [
        {
          "command": "react-transformer.warp_it"
        },
        {
          "command": "react-transformer.swap_with_next_sibling"
        },
        {
          "command": "react-transformer.create_forward"
        },
        {
          "command": "react-transformer.remove_wrapper"
        },
        {
          "command": "react-transformer.remove_all"
        },
        {
          "command": "react-transformer.create_ampersand_expression"
        },
        {
          "command": "react-transformer.create_conditional_expression"
        },
        {
          "command": "react-transformer.remove_children"
        }
      ]
    },
    "commands": [
      {
        "command": "react-transformer.warp_it",
        "title": "用新标签包裹（默认：Fragment）"
      },
      {
        "command": "react-transformer.swap_with_next_sibling",
        "title": "与下一个兄弟节点交换"
      },
      {
        "command": "react-transformer.create_forward",
        "title": "创建 forward ref"
      },
      {
        "command": "react-transformer.remove_wrapper",
        "title": "移除包裹标签"
      },
      {
        "command": "react-transformer.remove_all",
        "title": "移除所有内容"
      },
      {
        "command": "react-transformer.create_ampersand_expression",
        "title": "创建 & 表达式"
      },
      {
        "command": "react-transformer.create_conditional_expression",
        "title": "创建条件表达式"
      },
      {
        "command": "react-transformer.remove_children",
        "title": "移除所有子节点"
      },
      {
        "command": "react-transformer.showRefactorMenu",
        "title": "显示重构菜单"
      },
      {
        "command": "react-transformer.show_machine_id",
        "title": "显示本机机器码"
      },
      {
        "command": "react-transformer.input_activation_code",
        "title": "输入激活码"
      },
      {
        "command": "react-transformer.show_activation_code",
        "title": "显示激活码"
      },
      {
        "command": "react-transformer.show_activation_status",
        "title": "显示激活码状态"
      }
    ]
  },
  "scripts": {
    "build": "npm run compile && vsce package",
    "vscode:publish": "npm run vscode:prepublish&& vsce publish patch",
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "echo 'void'",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/generator": "^7.25.0",
    "@babel/parser": "^7.25.0",
    "@babel/template": "^7.25.0",
    "@babel/traverse": "^7.25.0",
    "@babel/types": "^7.25.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.6",
    "@types/lodash": "^4.17.17",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/node-rsa": "^1.1.4",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "macaddress": "^0.5.3",
    "node-rsa": "^1.1.1"
  }
}
