{
  "name": "react-transformer",
  "displayName": "react-transformer",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-transformer/react-transformer.git"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [],
    "menus": {
      "editor/context": [
        {
          "command": "react-transformer.warp_it"
        },
        {
          "command": "react-transformer.swap_with_next_sibling"
        },
        {
          "command": "react-transformer.create_forward"
        },
        {
          "command": "react-transformer.remove_wrapper"
        },
        {
          "command": "react-transformer.remove_all"
        },
        {
          "command": "react-transformer.create_ampersand_expression"
        },
        {
          "command": "react-transformer.create_conditional_expression"
        },
        {
          "command": "react-transformer.remove_children"
        }
      ]
    },
    "commands": [
      {
        "command": "react-transformer.warp_it",
        "title": "Wrap with new tag (default: Fragment)"
      },
      {
        "command": "react-transformer.swap_with_next_sibling",
        "title": "Swap with next sibling"
      },
      {
        "command": "react-transformer.create_forward",
        "title": "Create forward"
      },
      {
        "command": "react-transformer.remove_wrapper",
        "title": "Remove wrapper"
      },
      {
        "command": "react-transformer.remove_all",
        "title": "Remove all"
      },
      {
        "command": "react-transformer.create_ampersand_expression",
        "title": "Create ampersand expression"
      },
      {
        "command": "react-transformer.create_conditional_expression",
        "title": "Create conditional expression"
      },
      {
        "command": "react-transformer.remove_children",
        "title": "Remove all children from tag"
      },
      {
        "command": "react-transformer.showRefactorMenu",
        "title": "Show Refactor Menu"
      }
    ]
  },
  "scripts": {
    "build": "npm run compile && vsce package",
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "echo 'void'",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/generator": "^7.25.0",
    "@babel/parser": "^7.25.0",
    "@babel/template": "^7.25.0",
    "@babel/traverse": "^7.25.0",
    "@babel/types": "^7.25.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.17",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21"
  }
}
